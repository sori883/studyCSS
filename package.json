{
  "name": "simplicss",
  "version": "0.5.2",
  "version_short": "0.5",
  "description": "css",
  "keywords": [
    "css"
  ],
  "main": "dist/css",
  "scripts": {
    "dist": "npm-run-all css-lint css-compile css-prefix css-minify",
    "css": "npm-run-all css-*",
    "css-lint": "stylelint \"scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --source-comments --precision 6 scss/ -o dist/css/",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/simplicss.min.css dist/css/simplicss.css && cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/simplicss-grid.min.css dist/css/simplicss-grid.css && cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/simplicss-reboot.min.css dist/css/simplicss-reboot.css",
    "css-copy": "cpx \"dist/css/simplicss.min.css\" \"site/assets/css/\" ",
    "js": "npm-run-all js-*",
    "js-lint": "eslint --cache --cache-location .cache/.eslintcache js/src build/",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "node build/build-plugins.js",
    "js-compile-plugins-coverage": "cross-env NODE_ENV=test node build/build-plugins.js",
    "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/simplicss.js.map,includeSources,url=simplicss.min.js.map\" --output dist/js/simplicss.min.js dist/js/simplicss.js",
    "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/simplicss.bundle.js.map,includeSources,url=simplicss.bundle.min.js.map\" --output dist/js/simplicss.bundle.min.js dist/js/simplicss.bundle.js",
    "js-test-karma": "karma start js/tests/karma.conf.js",
    "js-test-karma-old": "cross-env USE_OLD_JQUERY=true npm run js-test-karma",
    "js-test-karma-bundle": "cross-env BUNDLE=true npm run js-test-karma",
    "js-test-karma-bundle-old": "cross-env BUNDLE=true USE_OLD_JQUERY=true npm run js-test-karma",
    "js-test-integration": "rollup --config js/tests/integration/rollup.bundle.js",
    "js-copy": "cpx \"dist/js/simplicss.min.js\" \"site/assets/js/\" ",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch scss/ --ext scss --exec \"npm run css\"",
    "watch-js": "nodemon --watch js/src --ext js --exec \"npm run js\"",
    "watch-js-study": "nodemon --watch js/src --ext js --exec \"npm run js-compile-plugins\"",
    "watch-server": "browser-sync start --server ./ --startPath site/"
  },
  "style": "dist/css/simplicss.css",
  "sass": "scss/simplicss.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sori883/simplicss.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "author": "sorinaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sori883/simplicss/issues"
  },
  "homepage": "https://github.com/sori883/simplicss#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "browser-sync": "^2.26.7",
    "clean-css-cli": "^4.3.0",
    "cpx": "^1.5.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "jquery": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-qunit": "^4.0.0",
    "karma-sinon": "^1.0.5",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "postcss-cli": "^6.1.3",
    "qunit": "^2.9.3",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "shx": "^0.3.2",
    "sinon": "^7.5.0",
    "stylelint": "^10.1.0",
    "stylelint-config-twbs-bootstrap": "^0.4.0",
    "uglify-js": "^3.7.6"
  },
  "dependencies": {},
  "peerDependencies": {
    "jquery": "1.9.1 - 3"
  }
}
